    "@types/antd": "^1.0.0",
    "@types/node": "18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@types/toastr": "^2.1.40",
