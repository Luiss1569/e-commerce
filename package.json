    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-lottie": "^1.2.3",
    "react-qr-code": "^2.0.8",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
